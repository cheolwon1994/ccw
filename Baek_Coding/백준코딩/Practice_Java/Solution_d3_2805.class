package workshop;

import java.util.Scanner;

public class Solution_d3_2805_ÃÖÃ¶¿ø {
	static int arr[][];
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int test = sc.nextInt();
		int result,num;
		int len;
		String str;
		for(int t=1;t<=test;t++) {
			result=0;
			num = sc.nextInt();
			arr = new int[num][num];
			for(int i=0;i<num;i++) {
				str = sc.next();
				for(int j=0;j<num;j++) {
					char c = str.charAt(j);
					arr[i][j]=c-'0';
				}
			}
			len = num/2;

			for(int i=0;i<num;i++) {
				for(int j=len;j<num-len;j++) {
					result+=arr[i][j];
				}
				if(i<num/2) 	len--;
				else			len++;
			}
			System.out.println("#"+t+" "+result);
		}
		sc.close();
	}
}
